{
    "manifest_version": 2,
    "name": "CSV Modifier",
    "version": "1.0",
    "description": "A simple extension to modify a CSV file.",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "https://code.jquery.com/"
    ],
    "background": {
        "scripts": [
            "content_script.js"
        ]
      },
    "web_accessible_resources": [
        "popup.html"
    ],
    "author": "AmanR"
}
